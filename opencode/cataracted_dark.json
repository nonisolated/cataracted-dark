    "syntaxKeyword": {
      "dark": "orchidPurple",
      "light": "orchidPurple"
    },
    "syntaxFunction": {
      "dark": "teal",
      "light": "teal"
    },
    "syntaxVariable": {
      "dark": "lightGray",
      "light": "lightGray"
    },
    "syntaxString": {
      "dark": "dustyRose",
      "light": "dustyRose"
    },
    "syntaxNumber": {
      "dark": "lavenderPurple",
      "light": "lavenderPurple"
    },
    "syntaxType": {
      "dark": "oliveGreen",
      "light": "oliveGreen"
    },
    "syntaxOperator": {
      "dark": "lightGray",
      "light": "lightGray"
    },
    "syntaxPunctuation": {
      "dark": "lightGray",
      "light": "lightGray"
    }
