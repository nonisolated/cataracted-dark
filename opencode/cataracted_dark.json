{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bgBase": "#1A1B20",
    "bgPanel": "#1F2229",
    "borderDefault": "#4A5660",
    "textMuted": "#607280",
    "textDefault": "#C2CED6",
    "accentWarm": "#BB936E",
    "primaryTeal": "#6A9492",
    "accentRose": "#C57883",
    "errorRed": "#D4646F",
    "warningYellow": "#CABD7E",
    "accentOlive": "#97A76C",
    "successGreen": "#94BB93",
    "accentViolet": "#AB74D3",
    "diffAddedBg": "#2C3B2F",
    "diffRemovedBg": "#3C2C2C",
    "number": "#8c6cc0"
  },
  "theme": {
    "primary": {
      "dark": "primaryTeal",
      "light": "primaryTeal"
    },
    "secondary": {
      "dark": "accentRose",
      "light": "accentRose"
    },
    "accent": {
      "dark": "accentWarm",
      "light": "accentWarm"
    },
    "error": {
      "dark": "errorRed",
      "light": "errorRed"
    },
    "warning": {
      "dark": "warningYellow",
      "light": "warningYellow"
    },
    "success": {
      "dark": "successGreen",
      "light": "successGreen"
    },
    "info": {
      "dark": "primaryTeal",
      "light": "primaryTeal"
    },
    "text": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "textMuted": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "background": {
      "dark": "bgBase",
      "light": "bgBase"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "backgroundElement": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "border": {
      "dark": "borderDefault",
      "light": "borderDefault"
    },
    "borderActive": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "borderSubtle": {
      "dark": "borderDefault",
      "light": "borderDefault"
    },
    "diffAdded": {
      "dark": "successGreen",
      "light": "successGreen"
    },
    "diffRemoved": {
      "dark": "errorRed",
      "light": "errorRed"
    },
    "diffContext": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "diffHunkHeader": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "diffHighlightAdded": {
      "dark": "successGreen",
      "light": "successGreen"
    },
    "diffHighlightRemoved": {
      "dark": "errorRed",
      "light": "errorRed"
    },
    "diffAddedBg": {
      "dark": "diffAddedBg",
      "light": "diffAddedBg"
    },
    "diffRemovedBg": {
      "dark": "diffRemovedBg",
      "light": "diffRemovedBg"
    },
    "diffContextBg": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "diffLineNumber": {
      "dark": "borderDefault",
      "light": "borderDefault"
    },
    "diffAddedLineNumberBg": {
      "dark": "diffAddedBg",
      "light": "diffAddedBg"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diffRemovedBg",
      "light": "diffRemovedBg"
    },
    "markdownText": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "markdownHeading": {
      "dark": "accentRose",
      "light": "accentRose"
    },
    "markdownLink": {
      "dark": "accentRose",
      "light": "accentRose"
    },
    "markdownLinkText": {
      "dark": "accentWarm",
      "light": "accentWarm"
    },
    "markdownCode": {
      "dark": "successGreen",
      "light": "successGreen"
    },
    "markdownBlockQuote": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "markdownEmph": {
      "dark": "warningYellow",
      "light": "warningYellow"
    },
    "markdownStrong": {
      "dark": "accentOlive",
      "light": "accentOlive"
    },
    "markdownHorizontalRule": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "markdownListItem": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "markdownListEnumeration": {
      "dark": "accentWarm",
      "light": "accentWarm"
    },
    "markdownImage": {
      "dark": "accentRose",
      "light": "accentRose"
    },
    "markdownImageText": {
      "dark": "accentWarm",
      "light": "accentWarm"
    },
    "markdownCodeBlock": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "syntaxComment": {
      "dark": "textMuted",
      "light": "textMuted"
    },
    "syntaxKeyword": {
      "dark": "accentViolet",
      "light": "accentViolet"
    },
    "syntaxFunction": {
      "dark": "primaryTeal",
      "light": "primaryTeal"
    },
    "syntaxVariable": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "syntaxString": {
      "dark": "accentRose",
      "light": "accentRose"
    },
    "syntaxNumber": {
      "dark": "number",
      "light": "number"
    },
    "syntaxType": {
      "dark": "accentOlive",
      "light": "accentOlive"
    },
    "syntaxOperator": {
      "dark": "textDefault",
      "light": "textDefault"
    },
    "syntaxPunctuation": {
      "dark": "textDefault",
      "light": "textDefault"
    }
  }
}
