{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "darkGray": "#1A1B20",
    "charcoalGray": "#1F2229",
    "steelBlue": "#4A5660",
    "slateBlue": "#687787",
    "lightGray": "#C2CED6",
    "camelBrown": "#BB936E",
    "teal": "#709C9B",
    "dustyRose": "#B76874",
    "coralRed": "#D4646F",
    "paleYellow": "#CABD7E",
    "oliveGreen": "#A2B279",
    "sageGreen": "#94BB93",
    "orchidPurple": "#AD79D2",
    "darkGreen": "#2C3B2F",
    "darkRed": "#3C2C2C",
    "lavenderPurple": "#8c6cc0"
  },
  "theme": {
    "primary": {
      "dark": "teal",
      "light": "teal"
    },
    "secondary": {
      "dark": "dustyRose",
      "light": "dustyRose"
    },
    "accent": {
      "dark": "camelBrown",
      "light": "camelBrown"
    },
    "error": {
      "dark": "coralRed",
      "light": "coralRed"
    },
    "warning": {
      "dark": "paleYellow",
      "light": "paleYellow"
    },
    "success": {
      "dark": "sageGreen",
      "light": "sageGreen"
    },
    "info": {
      "dark": "teal",
      "light": "teal"
    },
    "text": {
      "dark": "lightGray",
      "light": "lightGray"
    },
    "textMuted": {
      "dark": "slateBlue",
      "light": "slateBlue"
    },
    "background": {
      "dark": "darkGray",
      "light": "darkGray"
    },
    "backgroundPanel": {
      "dark": "charcoalGray",
      "light": "charcoalGray"
    },
    "backgroundElement": {
      "dark": "charcoalGray",
      "light": "charcoalGray"
    },
    "border": {
      "dark": "steelBlue",
      "light": "steelBlue"
    },
    "borderActive": {
      "dark": "lightGray",
      "light": "lightGray"
    },
    "borderSubtle": {
      "dark": "steelBlue",
      "light": "steelBlue"
